{
  "name": "tecsafe/jwt-sdk",
  "description": "The SDK for token parsing and validation against the JWKS. Also contains the types of token bodies.",
  "type": "library",
  "license": "MIT",
  "require": {
    "wol-soft/php-json-schema-model-generator-production": "^0.19.0",
    "firebase/php-jwt": "^6.10",
    "ext-json": "*",
    "ext-mbstring": "*",
    "psr/simple-cache": "^3",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.1"
  },
  "require-dev": {
    "wol-soft/php-json-schema-model-generator": "^0.24.0",
    "phpunit/phpunit": "^11.1",
    "symfony/cache": "^7.1",
    "symfony/http-client": "^7.1",
    "nyholm/psr7": "^1.8",
    "phpstan/phpstan": "^1.11",
    "friendsofphp/php-cs-fixer": "^3.58",
    "symfony/var-dumper": "^7.1"
  },
  "scripts": {
    "test": "@php vendor/bin/phpunit"
  },
  "autoload": {
    "psr-4": {
      "Tecsafe\\OFCP\\JWT\\": "src/php/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tecsafe\\OFCP\\JWT\\Test\\": "test/php/"
    }
  },
  "minimum-stability": "stable"
}
